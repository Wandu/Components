{
    "name": "wandu/router",
    "description": "FastRouter with PSR-7 Wrapper Library.",
    "keywords": ["wandu", "router", "psr7", "middleware"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Wandu\\Router\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wandu\\Router\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.0.0",
        "psr/http-message": "^1",
        "nikic/fast-route": "^0.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2",
        "mockery/mockery": "^0.9.4"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "suggest": {
        "wandu/http": "PSR-7 implementation"
    }
}

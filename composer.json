{
    "name": "wandu/framework",
    "description": "Wandu Framework",
    "keywords": ["wandu", "framework", "psr7"],
    "license": "MIT",
    "authors": [
        {
            "name": "Changwan Jun",
            "email": "im@wani.kr"
        }
    ],
    "replace": {
        "wandu/di": "self.version",
        "wandu/http": "self.version",
        "wandu/router": "self.version"
    },
    "bin": ["bin/wandu"],
    "require": {
        "php": ">=7.0.0",
        "psr/http-message": "^1",
        "container-interop/container-interop": "^1.1",
        "nikic/fast-route": "^0.6",
        "wandu/reflection": "^0.0.1",
        "symfony/console": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "Wandu\\": "src/Wandu/",
            "Publ\\": "src/Publ/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wandu\\": "tests/Wandu/",
            "Publ\\": "tests/Publ/"
        }
    }
}

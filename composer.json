{
    "name": "wandu/framework",
    "description": "Wandu Framework",
    "keywords": ["wandu", "framework", "psr7"],
    "license": "MIT",
    "bin": ["bin/wandu"],
    "require": {
        "php": ">=7.0.0",
        "psr/http-message": "^1",
        "container-interop/container-interop": "^1.1",
        "nikic/fast-route": "^0.6",
        "wandu/reflection": "^0.0.1",
        "wandu/fastelper": "^0.1.0",
        "symfony/console": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3",
        "mockery/mockery": "^0.9.4",
        "pda/pheanstalk": "^3.1",
        "aws/aws-sdk-php": "^3.18"
    },
    "autoload": {
        "psr-4": {
            "Wandu\\": "src/Wandu/",
            "Publ\\": "src/Publ/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wandu\\": "tests/Wandu/",
            "Publ\\": "tests/Publ/"
        }
    },
    "replace": {
        "wandu/caster": "self.version",
        "wandu/compiler": "self.version",
        "wandu/di": "self.version",
        "wandu/http": "self.version",
        "wandu/q": "self.version",
        "wandu/router": "self.version",
        "wandu/tempy": "self.version"
    },
    "suggest": {
        "aws/aws-sdk-php": "for SQS queue in Wandu/Q",
        "pda/pheanstalk": "for Beanstalkd queue in Wandu/Q"
    }
}

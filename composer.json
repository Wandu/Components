{
    "name": "wandu/compiler",
    "description": "PHP Base Compiler.",
    "keywords": ["compiler", "lexer", "wandu"],
    "license": "MIT",
    "authors": [
        {
            "name": "Changwan Jun",
            "email": "im@wani.kr"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "wandu/fastelper": "^0.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.5.*",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {

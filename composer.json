{
    "name": "wandu/di",
    "description": "Dependency Injection Container. Support Auto-Resolve and Auto-Wiring.",
    "keywords": ["di", "ioc", "auto resolve", "auto wiring", "dependency injection", "wandu"],
    "license": "MIT",
    "authors": [
        {
            "name": "Changwan Jun",
            "email": "im@wani.kr"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "wandu/reflection": "^0.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "4.5.*",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "Wandu\\DI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wandu\\DI\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
